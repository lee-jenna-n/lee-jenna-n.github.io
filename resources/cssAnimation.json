    {
        "name":"Rainbow Heart",
        "html":"<i class=\"fa fa-heart\"></i>",
        "css":[
            "@keyframes colors {",
                "0% {color: red;}",
                "12% {color: orange;}",
                "24% {color: yellow;}",
                "36% {color: green;}",
                "48% {color: blue;}",
                "60% {color: indigo;}",
                "72% {color: purple;}",
                "84% {color: deeppink;}",
                "93% {color: red;}",
            "}",
            ".fa-heart {",
               " color: red;",
                "font-size: 30px;",
                "@include animation(colors, 4s, infinite);",
            "}"
        ]
    },
